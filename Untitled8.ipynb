   "source": [
    "from sklearn.cluster import MeanShift\n",
    "from sklearn.datasets import load_digits\n",
    "\n",
    "data = load_digits()\n",
    "X = data.data\n",
    "\n",
    "ms = MeanShift(bandwidth=10)\n",
    "labels = ms.fit_predict(X)\n",
    "\n",
    "print(labels[:10])"
   ]
